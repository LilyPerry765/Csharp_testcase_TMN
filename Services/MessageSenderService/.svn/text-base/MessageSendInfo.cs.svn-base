using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace MessageSenderService
{
	class MessageSendInfo
	{
		public MessageSendInfo(Guid circuitnumber, DateTime currentdate)
		{
			this.CircuitNumber = circuitnumber;
			this.CurrentDate = currentdate;
		}

		public Guid CircuitNumber
		{
			get;
			set;
		}

		public DateTime CurrentDate
		{
			get;
			set;
		}
	}
}
